{
    "manifest_version": 3,
    "name": "NexusMods No Premium",
    "version": "1.2.0",
    "description": "Hides premium advertisements on Nexus Mods",
    "permissions": ["activeTab"],
    "content_scripts": [{
        "matches": ["*://www.nexusmods.com/*"],
        "js": ["nopremium.js"],
        "run_at": "document_end"
    }]
}